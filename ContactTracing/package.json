{
  "name": "ContactTracing",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "ios": "react-native run-ios",
    "start": "yarn run relay; concurrently --kill-others --names \"react-scripts,relay\" \"react-native start\" \"yarn run relay --watch\"",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "npx jetify",
    "relay": "relay-compiler --src ./src --schema ../schema.graphql --language typescript --artifactDirectory ./src/__generated__"
  },
  "dependencies": {
    "babel-plugin-relay": "^9.0.0",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-background-actions": "^2.2.0",
    "react-native-background-timer": "^2.2.0",
    "react-native-ble-plx": "https://github.com/RichardLindhout/react-native-ble-plx",
    "react-native-contact-tracing": "^1.11.0",
    "react-native-device-info": "^5.5.5",
    "react-native-navigation": "^6.4.0",
    "react-native-paper": "^3.8.0",
    "react-native-permissions": "^2.0.10",
    "react-native-simple-crypto": "https://github.com/RichardLindhout/react-native-simple-crypto",
    "react-native-track-player": "^1.2.2",
    "react-native-vector-icons": "^6.6.0",
    "react-relay": "^9.0.0",
    "react-translated": "^2.5.0",
    "realm": "^5.0.3",
    "rn-secure-storage": "^2.0.4",
    "use-debounce": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/i18n-js": "^3.0.2",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-native": "^0.62.2",
    "@types/react-native-background-timer": "^2.0.0",
    "@types/react-relay": "^7.0.3",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^25.3.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "graphql": "^15.0.0",
    "graphql-compiler": "^1.7.0",
    "jest": "^25.3.0",
    "jetifier": "^1.6.5",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.11.0",
    "relay-compiler": "^9.0.0",
    "relay-compiler-language-typescript": "^12.0.0",
    "relay-config": "^9.0.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
